.numbers {
    display: grid;
    grid-template-columns: repeat(12, 1fr);
    font-size: 16px;
    min-width: 800px;
    text-align: center;
    line-height: 24px;
    font-size: 12px;
    font-weight: 600;
}

.inlays {
    display: grid;
    grid-template-columns: repeat(12, 1fr);
    min-width: 800px;
    text-align: center;
    line-height: 16px;
}

.dot {
    height: 5px;
    width: 5px;
    background: #000;
    margin: 5px auto;
    border-radius: 50%;
}
